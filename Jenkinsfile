        stage('测试') {
             steps {
                echo "测试..."
             }
        }

        //3、打包
        stage('打包') {
            steps {
                echo "打包..."
            }
        }

        //4、部署
        stage('部署') {
            steps {
                echo "部署..."
            }
        }
    }